      <min value="1"/>
      <max value="*"/>
    </element>
    <element id="asEntityIdentifier.classCode">
      <path value="asEntityIdentifier.classCode"/>
      <representation value="xmlAttr"/>
      <min value="0"/>
      <max value="1"/>
